{
  "name": "platzigram-api",
  "version": "1.0.0",
  "description": "Platzigram REST API",
  "scripts": {
    "lint": "standard",
    "test": "npm run lint && ava"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime",
      "transform-async-to-generator"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jluisacosta/platzigram-api.git"
  },
  "author": "J Luis Acosta <jluis.acosta@outlook.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jluisacosta/platzigram-api/issues"
  },
  "homepage": "https://github.com/jluisacosta/platzigram-api#readme",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-eslint": "^7.1.1",
    "babel-register": "^6.18.0",
    "standard": "^8.6.0",
    "test-listen": "^1.0.0"
  },
  "dependencies": {
    "babel-runtime": "^6.18.0",
    "http-hash": "^2.0.0",
    "micro": "^4.1.1",
    "request-promise": "^4.1.1",
    "uuid-base62": "^0.1.0"
  }
}
